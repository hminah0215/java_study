class StudentTest 
{
	public static void main(String[] args) 
	{
		String []name = {"현민아","최영수","한주련","최봉현","이희재","이원우","이수인" };
		int age[] = { 29,28,32,29,25,31,28 };
		

		for(	int i=0	; i < name.length	; i++	){
			System.out.println( name[i] + "\t" + age[i] ); //\t 탭 설정된만큼 빈칸출력
		}

			//나이가 낮은순으로 정렬합니다. 

			for( int i=0 ; i < age.length ; i++ ){
			for (int j = i+1 ; j < age.length ; j++ ){
				if( age[j] < age[i] ){  
					int temp ;  
					temp = age[i];
				     age[i] = age[j];
					 age[j] = temp;
			
					String temp2 = name[i];
					name[i] = name[j];
					name[j] = temp2;
			
			}
		} 
	}		System.out.println();
			System.out.println("***나이순정렬***");
			for(	int i=0	; i < name.length	; i++	){
			System.out.println( +name[i] + "\t" + age[i] ); //\t 탭 설정된만큼 빈칸출력
			
		}
	}
}
